#!/usr/bin/env ruby
#coding: utf-8

require 'thor'
require 'jj'

class Command < Thor
  default_command :to_timestamp

  desc 'to_date', '2014-01-01 00:00:00 to_date'
  option :date, :type => :string, :aliases => :'-t', :desc => "timestamp"
  def to_date
    JJ.new.sample
    # JJ.new.todate(params[:date])
  end

end

Command.start
